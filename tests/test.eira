spell isPrime(a: NumWeave):: TruthWeave {
    fate a <= 1 {
       release false;
     }

    mark b = 2;
     while(b < a) {
        fate (a%b == 0) {
            release false;
        }
        b = b+1;
     }

    release true;
}

mark x = 0;
mark count = 0;
while(x <= 10000) {
    
    fate cast isPrime with x {
        //chant x;
        count = count + 1;
    }

    x = x+1;
}

chant "Prime nums:";
chant count;